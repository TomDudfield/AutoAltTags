<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <events>
      <event name="item:saved">
        <handler type="AutoAltTags.SetAutoAltTag, AutoAltTags" method="OnItemSaved" patch:before="processor[@type='Sitecore.Tasks.ItemEventHandler, Sitecore.Kernel']">
          <apiKey></apiKey>
          <defaultApiRoot>https://westcentralus.api.cognitive.microsoft.com/vision/v1.0</defaultApiRoot>
        </handler>
      </event>
    </events>
  </sitecore>
</configuration>
